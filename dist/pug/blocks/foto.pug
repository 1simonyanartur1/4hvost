mixin foto()
	.foto 
		.foto__div
			.foto__pictures

				.foto__zoom 
					<svg width="46" height="46" viewBox="0 0 46 46" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
					<path d="M45.8267 0.802856H0.728516V45.9011H45.8267V0.802856Z" fill="url(#pattern0)"/>
					<defs>
					<pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
					<use xlink:href="#image0" transform="scale(0.00628931)"/>
					</pattern>
					<image id="image0" width="159" height="159" xlink:href="data:image/png;base64,"/>
					</defs>
					</svg>
				.foto__images
					
					a(href=img+"slider/goods_1.jpg" data-fancybox="image").foto__item
						img(src=img+"slider/goods_1.jpg", alt="")
					a(href=img+"slider/goods_2.jpg" data-fancybox="image").foto__item
						img(src=img+"slider/goods_2.jpg", alt="")

					.foto__item.foto__video.lazy(data-src="https://img.youtube.com/vi/kdhCfhxZhrk/maxresdefault.jpg ")
						a(data-fancybox href="https://www.youtube.com/watch?v=kdhCfhxZhrk") 
							.foto__play 
								img.lazy(data-src=img+"play_button.png")
					a(href=img+"slider/goods_3.jpg" data-fancybox="image").foto__item
						img(src=img+"slider/goods_3.jpg", alt="")
					a(href=img+"slider/goods_1.jpg" data-fancybox="image").foto__item
						img(src=img+"slider/goods_1.jpg", alt="")
				
				.foto__slider
					img(src=img+"slider/goods_1.jpg", alt="")
					img(src=img+"slider/goods_2.jpg", alt="")

					.foto__video.foto__video_mini.lazy(data-src="https://img.youtube.com/vi/kdhCfhxZhrk/maxresdefault.jpg ")
						
							.foto__play 
								img.lazy(data-src=img+"play_button.png")

					img(src=img+"slider/goods_3.jpg", alt="")
					img(src=img+"slider/goods_1.jpg", alt="")

				.foto__controls
					.foto__arrows
				